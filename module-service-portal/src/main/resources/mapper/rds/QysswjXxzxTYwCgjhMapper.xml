                and createtime &lt;= #{qsrq}
                -- to_date(#{zzrq},'yyyy-MM-dd HH:mm:ss')
            </if>
        </where>

        <!--<if test="pageNum>0">-->
        <!--limit ${(pageNum-1)*pageSize},${pageNum*pageSize}-->
        <!--</if>-->


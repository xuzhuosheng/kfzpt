        select
          id, cgjhmc, createtime, creater, zt, bz
        from aly_kfkjpx_test.qysswj_xxzx_t_yw_cgjh
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="QysswjXxzxTYwCgjhMap">
        select
        id, cgjhmc, createtime, creater, zt, bz
        from aly_kfkjpx_test.qysswj_xxzx_t_yw_cgjh
        <where>
            <if test="id != null">
